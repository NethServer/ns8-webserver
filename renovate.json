{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["local>NethServer/.github:ns8"],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["php"],
      "matchPackagePatterns": ["php:.*-fpm.*"],
      "rangeStrategy": "pin",
      "bumpTypes": ["patch"]
    }
  ]
}
